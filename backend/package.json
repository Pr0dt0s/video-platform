{
    "name": "backend",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "type": "commonjs",
    "scripts": {
        "sequelize-cli": "sequelize-cli",
        "test": "jest",
        "test:watch": "jest --watchAll",
        "type-check": "tsc -p ./tsconfig.json",
        "type-check:dev": "tsc --watch -p ./tsconfig.dev.json",
        "build": "swc --out-dir dist ./src",
        "build:dev": "swc --out-dir dist -w --sync ./src",
        "watch": "nodemon ./dist",
        "predb:migrate": "npm run build",
        "db:migrate": "sequelize-cli db:migrate",
        "dev": "concurrently -n build,tsc,dev \"npm run build:dev\" \"npm run type-check:dev\" \"npm run watch\""
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "bcrypt": "^5.1.0",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-request-id": "^3.0.0",
        "express-session": "^1.17.3",
        "helmet": "^7.0.0",
        "jsonwebtoken": "^9.0.1",
        "morgan": "^1.10.0",
        "nodemon": "^3.0.1",
        "passport": "^0.6.0",
        "passport-jwt": "^4.0.1",
        "passport-local": "^1.0.0",
        "pg": "^8.11.2",
        "pg-hstore": "^2.3.4",
        "sequelize": "^6.32.1",
        "zod": "^3.22.0"
    },
    "devDependencies": {
        "@swc/cli": "^0.1.62",
        "@swc/core": "^1.3.76",
        "@swc/jest": "^0.2.29",
        "@types/bcrypt": "^5.0.0",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/express-session": "^1.17.7",
        "@types/jest": "^29.5.3",
        "@types/morgan": "^1.9.4",
        "@types/node": "^20.4.10",
        "@types/passport": "^1.0.12",
        "@types/passport-jwt": "^3.0.9",
        "@types/passport-local": "^1.0.35",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^6.3.0",
        "@typescript-eslint/parser": "^6.3.0",
        "chokidar": "^3.5.3",
        "concurrently": "^8.2.0",
        "eslint": "^8.47.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.0",
        "jest": "^29.6.2",
        "lint-staged": "^13.3.0",
        "prettier": "3.0.1",
        "sequelize-cli": "^6.6.1",
        "supertest": "^6.3.3",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.6"
    },
    "lint-staged": {
        "*.{js,ts,cjs,mjs}": [
            "eslint --cache --fix",
            "prettier --write"
        ],
        "*.{md}": "prettier --write",
        "{package.json,.swcrc,tsconfig*.json}": "prettier --write"
    }
}
